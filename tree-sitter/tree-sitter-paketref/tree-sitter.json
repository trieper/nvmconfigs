{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "paketref",
      "camelcase": "Paketref",
      "title": "Paketref",
      "scope": "source.paketref",
      "file-types": [
        "paketref"
      ],
      "injection-regex": "^paketref$",
      "class-name": "TreeSitterPaketref"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "paket.references",
    "authors": [
      {
        "name": "trieper"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-paketref"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}